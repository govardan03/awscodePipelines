{
    "executionRoleArn":"arn:aws:iam::account_ID:role/ecsTaskExecutionRole",
    "containerDefinitions":[
      {
          "name":"AWS-CodePipeline",
          "image":"029298191954.dkr.ecr.us-east-1.amazonaws.com/docker:latest",
          "essential": true,
          "postmapping":[
              {
                  "hostPost": 8080,
                  "protocol": "tcp",
                  "containerPort": 8080
              }
         ]
      }
     ],
    "requiresCompatibilities": [
        "EC2"
     ],
    "networkMode": "Bridge",
    "cpu": "256",
    "memory": "512",
    "family": "docker"
}
